<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import org.kordamp.ikonli.javafx.FontIcon?>

<fx:root type="VBox" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="label" text="Label" />
      <HBox minWidth="100.0">
         <children>
            <TextField fx:id="textField" prefColumnCount="4" HBox.hgrow="ALWAYS" />
            <Button fx:id="button" mnemonicParsing="false">
               <graphic>
                  <FontIcon fx:id="buttonIcon" iconLiteral="far-square" />
               </graphic>
            </Button>
         </children>
      </HBox>
   </children>
</fx:root>
